<div class="posts-index">
    <div class="container">
        <ul class="skill-list jscroll">
            <% @products.each do |product| %>
             <section class="card">
                     <div class="card-content">
                       <h1 class="card-title"><%= product.name %></h1>
                     </div>
             </section>
           <% end %>
           <%= paginate @products %>
        </ul>

    </div>
</div>